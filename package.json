{
  "name": "file-uploader-the-odin-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x bun --env-file=.env.development.local src/server/main.ts",
    "start": "NODE_ENV=production bun --env-file=.env.production.local src/server/main.ts",
    "build": "vite build",
    "prisma:dev": "bunx prisma migrate dev",
    "prisma:dev:migrate": "bun --env-file=.env.development.local prisma:dev --name",
    "prisma:generate": "bunx prisma generate",
    "prisma:generate:watch": "bunx prisma generate --watch"
  },
  "dependencies": {
    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "valibot": "^1.0.0-beta.3",
    "vite-express": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@tanstack/eslint-plugin-router": "^1.79.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11"
  }
}
